{
  "name": "keller-ruiz-wedding",
  "version": "1.0.0",
  "description": "Leslie and Tony's Wedding Website",
  "main": "index.js",
  "scripts": {
    "sass": "sass sass/styles.scss css/styles.css --style=compressed",
    "sass:watch": "sass sass/styles.scss css/styles.css --style=compressed --watch",
    "minify": "esbuild js/scripts.js --minify --outfile=js/scripts.min.js",
    "build": "npm run sass && npm run minify",
    "dev": "npm run sass:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byte-sized-mischief/keller-ruiz-wedding-website.git"
  },
  "keywords": [
    "wedding",
    "website"
  ],
  "author": "Jackie",
  "license": "MIT",
  "homepage": "https://github.com/byte-sized-mischief/keller-ruiz-wedding-website",
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "esbuild": "^0.25.5",
    "sass": "^1.69.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "animate.css": "^4.1.1",
    "waypoints": "^4.0.1"
  }
}
